{
  "name": "picpay/sonserina-pay",
  "description": "This is a test for PHP Language on PicPay.",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-0": {
      "App": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.x-dev",
    "infection/infection": "dev-master",
    "phpro/grumphp": "dev-master",
    "squizlabs/php_codesniffer": "4.0.x-dev",
    "phpmd/phpmd": "dev-master",
    "povils/phpmnd": "^2.4"
  },
  "scripts": {
    "test": "vendor/bin/phpunit --coverage-text",
    "check:quality": "vendor/bin/grump run",
    "infection": "vendor/bin/infection --min-msi=100 --min-covered-msi=100"
  }
}
